set nocompatible
set nu
set expandtab
set tabstop=4
colorscheme brogrammer
set autoindent
:highlight LineNr ctermfg=yellow
hi TODO cterm=NONE ctermfg=black ctermbg=gray
hi NOTE cterm=NONE ctermfg=black ctermbg=gray
hi FIXME cterm=NONE ctermfg=black ctermbg=gray
hi HACK cterm=NONE ctermfg=black ctermbg=gray
hi QUESTION cterm=NONE ctermfg=black ctermbg=gray
hi Search cterm=NONE ctermfg=black ctermbg=yellow
